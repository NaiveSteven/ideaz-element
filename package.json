{
  "name": "ideaz-element",
  "type": "module",
  "version": "0.0.1-beta.9",
  "packageManager": "pnpm@7.30.0",
  "license": "MIT",
  "homepage": "https://naivesteven.github.io/ideaz-element/",
  "keywords": [
    "element-plus",
    "pro-components",
    "components-library",
    "tsx",
    "Vue3",
    "TypeScript",
    "Vite"
  ],
  "main": "lib/index.cjs",
  "module": "es/index.mjs",
  "types": "dist/element/index.d.ts",
  "style": "dist/theme-chalk/index.css",
  "files": [
    "CHANGELOG.md",
    "dist",
    "es",
    "lib",
    "style",
    "theme-chalk",
    "types.d.ts"
  ],
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs --base /ideaz-element/",
    "docs:serve": "vitepress serve docs",
    "dev3": "cl switchVue 3 && vite --config ./demo/vue3/vite.config.ts --open",
    "dev2.7": "cl switchVue 2.7 && vite --config ./demo/vue2.7/vite.config.ts --open",
    "dev2.6": "cl switchVue 2.6 && vite --config ./demo/vue2.6/vite.config.ts --open",
    "build:theme": "gulp --gulpfile build/gulpfile.mjs",
    "build": "pnpm clean && tsx build/build-components.ts && tsx build/build-types.ts && pnpm build:theme",
    "clean": "rimraf lib && rimraf dist && rimraf es && rimraf theme-chalk",
    "lint": "eslint .",
    "lint:style": "stylelint --cache \"**/*.{vue,scss}\"",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I"
  },
  "peerDependencies": {
    "@vue/composition-api": ">=1",
    "vue": "2||3"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@vueuse/core": "latest",
    "lodash-es": "latest",
    "lodash-unified": "latest",
    "sortablejs": "^1.15.0",
    "vue-draggable-plus": "^0.2.4",
    "vue-global-config": "latest"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.1",
    "@element-plus/icons-vue": "latest",
    "@iconify-json/carbon": "^1.1.16",
    "@ideaz/directives": "workspace:*",
    "@ideaz/hooks": "workspace:*",
    "@ideaz/locale": "workspace:*",
    "@ideaz/shared": "workspace:*",
    "@ideaz/theme-chalk": "workspace:*",
    "@ideaz/utils": "workspace:*",
    "@types/gulp": "^4.0.17",
    "@types/gulp-autoprefixer": "^0.0.37",
    "@types/gulp-clean-css": "^4.3.4",
    "@types/gulp-sass": "^5.0.4",
    "@types/lodash-es": "latest",
    "@types/node": "latest",
    "@types/sortablejs": "^1.15.3",
    "@unocss/eslint-config": "^0.58.3",
    "@unocss/reset": "^0.50.6",
    "@vexip-ui/prettier-config": "^0.2.0",
    "@vexip-ui/stylelint-config": "^1.0.0",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vitejs/plugin-vue-jsx": "^4.0.0",
    "@vitepress-demo-preview/component": "^2.3.2",
    "@vitepress-demo-preview/plugin": "^1.2.2",
    "@vue/compiler-sfc": "^3.4.27",
    "@vue/test-utils": "^2.0.0",
    "element-plus": "^2.7.6",
    "element-resize-detector": "latest",
    "eslint": "^8.36.0",
    "eslint-plugin-format": "^0.1.0",
    "flexsearch": "^0.7.31",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^9.0.0",
    "gulp-clean-css": "^4.3.0",
    "gulp-sass": "^5.1.0",
    "ideaz-element": "workspace:*",
    "jsdom": "^21.1.1",
    "lint-staged": "^13.2.0",
    "lossless-json": "latest",
    "magic-string": "^0.30.10",
    "pnpm": "^7.30.0",
    "prettier": "^2.8.8",
    "sass": "^1.69.5",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^16.1.0",
    "stylelint-prettier": "^5.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.2",
    "unocss": "^0.50.6",
    "unplugin-auto-import": "latest",
    "unplugin-vue-macros": "^0.11.2",
    "vite": "^5.2.13",
    "vite-plugin-dts": "^3.6.0",
    "vitepress": "1.0.1",
    "vitepress-plugin-search": "1.0.4-alpha.21",
    "vitepress-theme-demoblock": "^2.0.2",
    "vitest": "latest",
    "vue": "3.4.27",
    "vue-router": "latest",
    "vue-tsc": "^1.2.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
